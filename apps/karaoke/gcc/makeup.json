{
  "root": {
    "version": "4",

    "boot":
    {
      "ram_addr_write": "0x860000",
      "ram_addr_run": "0x860001",
      "boot_addr": "0x60",
      "norflash": {
        "norf_line": "0",
        "norf_baud": "9"
      },
      "doDebug": "1",
      "loader": {
        "type": "HLKJ",
        "path": "$(ROOT_PATH)/bootloader/bin/g24_loader.bin"
      }
    },

    "partitions":
    {
      "firmware":
      {
        "type": "FIRM",
        "path": "$(ROOT_PATH)/apps/karaoke/gcc/.out/karaoke.bin",
        "loadLength": "0x1000",
        "loadToRam": "0x804c40",
        "runFromRam": "0x804c41"
      },

      "others":
      [
        {
          "type": "TONE",
          "path": "$(ROOT_PATH)/apps/karaoke/app_config/$(BOARD)/bin/tone.bin"
        },
		{
          "type": "AENC",
          "path": "$(ROOT_PATH)/apps/karaoke/app_config/$(BOARD)/bin/default.aenc"
		},

        {
          "type": "EQ2P",
          "path": "$(ROOT_PATH)/apps/karaoke/app_config/$(BOARD)/bin/eq_profile_customer.eq2"
        }
		
      ],
     
	  "psm":
      {
        "type": "PSMP",
        "addr":"0xFC000",
        "length":"0x4000"
      }
    }
  }
}

